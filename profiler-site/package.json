{
  "name": "profiler-site",
  "version": "1.0.0",
  "description": "Website for developers to build their own portfolio, resume, showcase...",
  "main": "index.js",
  "scripts": {
    "lint": "standard",
    "lint:fix": "standard --fix",
    "lint:staged": "lint-staged",
    "precommit": "lint-staged",
    "test": "jest --silent --colors --noStackTrace --passWithNoTests",// --watch vem por padrão com -o, que serve pra rodar só quando tem mudanças nos arquivos testados
    "test:unit": "npm test -- --watch",
    "test:staged": "npm test -- --findRelatedTests", // findRelatedTests faz o mesmo que o --watch, só roda o teste que precisa
    "test:ci": "npm test -- --coverage"
  },
  "standard": {
    "env": ["jest"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LuizAraujo2020/UCB-Projeto-1_2024.git"
  },
  "keywords": [
    "web",
    "js",
    "html",
    "css",
    "node",
    "express",
    "ihc",
    "ucb",
    "pair",
    "programming",
    "2024"
  ],
  "author": "Juliana Pires, Luiz Araujo, Natalia Carvalho",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LuizAraujo2020/UCB-Projeto-1_2024/issues"
  },
  "homepage": "https://github.com/LuizAraujo2020/UCB-Projeto-1_2024#readme",
  "dependencies": {
    "express": "^4.18.2",
    "mustache-express": "^1.3.2",
    "normalize.css": "^8.0.1"
  },
  "devDependencies": {
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "standard": "^17.1.0"
  }
}
